{
  "manifest_version": 2,
  "name": "GitHub Issue Point",
  "version": "1.0.0",
  "description": "GitHubのIssueに追加したポイントを足し算するエクステンション",
  "icons": {
    "128": "images/icon.jpg"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["stylesheets/application.css"],
      "js": [
        "javascripts/vendor/jquery-2.2.1.min.js",
        "javascripts/vendor/lodash-4.5.1.min.js",
        "javascripts/application.js"
      ]
    }
  ]
}
